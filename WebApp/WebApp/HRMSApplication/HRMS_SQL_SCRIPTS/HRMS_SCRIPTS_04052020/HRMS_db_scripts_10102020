ALTER TABLE Timesheet_Request_Form ADD exittime varchar(100);
ALTER TABLE Timesheet_Request_Form ADD entrytime varchar(100); 

CREATE table calender (mydate date);

declare @startdate as date = '2015-01-01'
WHILE(@startdate!=DATEADD(D, 0, DATEDIFF(D, 0, '2050-12-31')))
begin
insert into calender VALUES(@startdate)
set @startdate=dateadd(day,1,@startdate)
end

update PLE_Type set CurrentYear= LTRIM(RTRIM(CurrentYear))

alter table Employees add photo varchar(5);
update employees set photo='No';

