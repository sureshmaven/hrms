    <script type="text/javascript" src="~/Areas/Payroll/Assets/libs/jquery/pdfmake.min.js"></script>
    <script type="text/javascript" src="~/Areas/Payroll/Assets/libs/jquery/vfs_fonts.js"></script>
    @*<script type="text/javascript" src="https://cdn.datatables.net/responsive/1.0.7/js/dataTables.responsive.min.js"></script>*@
    <!-- scripts for data tables -->
    <!-- start stylesheet -->
    <link href="~/Areas/Payroll/Assets/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Areas/Payroll/Assets/css/style.css" rel="stylesheet" />
    <link href="~/Areas/Payroll/Assets/libs/jquery/toastr.min.css" rel="stylesheet" />
    <link href="~/Areas/Payroll/Assets/css/jquery.datatables.min.css" rel="stylesheet" />
    <link href="~/Areas/Payroll/Assets/css/buttons.datatables.min.css" rel="stylesheet" />
    <link href="~/Areas/Payroll/Assets/css/report-style.css" rel="stylesheet" />
    <link href="~/Areas/Payroll/Assets/css/bootstrap-datetimepicker.css" rel="stylesheet" />
    <link href="~/Areas/Payroll/Assets/css/bootstrap-datepicker3.min.css" rel="stylesheet" />
    <link href="~/Areas/Payroll/Assets/css/easy-responsive-tabs.css" rel="stylesheet" />
    <link href="~/Areas/Payroll/Assets/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Areas/Payroll/Assets/css/popuo-box.css" rel="stylesheet" />
    <!--<link rel="stylesheet" type="text/css" href="http://cdn.webrupee.com/font">-->
