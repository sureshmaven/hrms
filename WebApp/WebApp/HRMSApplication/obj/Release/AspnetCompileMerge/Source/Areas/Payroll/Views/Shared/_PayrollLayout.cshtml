    <script type="application/x-javascript">
        addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);

        function hideURLbar() {
            window.scrollTo(0, 1);
        }

    </script>
    <!-- //for-mobile-apps -->

    <style>
      
    </style>

</head>
<body>
    <div style="display: none;" id="dvLoading"></div>
    <div style="display: none;" class="ui-widget-overlay  overlayforaddNote"></div>
    @{
        string rolepages = ViewBag.RolePages;
    }
    <div id="menuheader" class="header">
        <div class="container-fluid">
            <div class="logo-nav">
                <div class="logo-nav-left1">
                    <div class="logo-nav-left">
                        <a href="#"><img src="~/Assets/images/logo.png" /></a>
                    </div>
                    <div class="right-menus">
                        <div class="header-grid">
                            <span class="site-title">Employee Payroll System</span>
                            <span class="site-title2">Payroll</span>
                            <div class="header-grid-left">
                                <ul>
                                    @*<li class="resp-none"><i class="glyphicon glyphicon-file" aria-hidden="true"></i><a href="@Url.Action("Policies", "Home")">Policies</a></li>*@

                                    @*<li><i class="glyphicon glyphicon-file" aria-hidden="true"></i><a href="@Url.Action("Terms", "Home")">Terms of Use</a></li>*@
                                    <li class="resp-none menu_heading"><i class="glyphicon glyphicon-earphone" aria-hidden="true"></i><a href="@Url.Action("Index", "Help")">Help</a></li>
                                    @{

                                        var lActiveEmployee = Session.GetDataFromSession<string>("EmpFullName");
                                        var ldesignation = Session.GetDataFromSession<string>("CurrDesig");
                                        var lActiveImage = Session.GetDataFromSession<string>("ActiveImage");

                                        <li class="hid-names"><img style="width:30px; height:30px;" src="~/uploads/@lActiveImage" /><strong> @lActiveEmployee <br /> <span class="hid-names"> @ldesignation </span></strong></li>

                                    }
                                    @*<li><strong> @ldesignation </strong></li>*@
                                    <li class="menu_heading"><i class="glyphicon glyphicon-log-in" aria-hidden="true"></i>@Html.RouteLink("Logout", "Default", new { action = "Index", controller = "Home" })</li>
                                </ul>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        @{
                            var controllername = (ViewContext.RouteData.Values["controller"] ?? "").ToString().ToLower();

                        }
                        <nav class="navbar navbar-default">

                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle collapsed navbar-toggle1" data-toggle="collapse" data-target="#bs-megadropdown-tabs">Menu <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
                            </div>
                            <div class="collapse navbar-collapse" id="bs-megadropdown-tabs">
                                <ul class="nav navbar-nav">
                                    <li class="@(controllername == "prdashboard" ? "active" : "")"><a href="@Url.Action("Index", "PRDashboard")"> <i class="fa fa-tachometer" aria-hidden="true"></i>Dashboard</a></li>
                                    <li class="@(controllername == "transaction" ? "active" : "")"><a href="@Url.Action("Index", "Transaction")"><i class="fa fa-exchange" aria-hidden="true"></i>Transactions</a></li>
                                    <li class="@(controllername.EndsWith("masters") ? "active" : "")"><a href="@Url.Action("Index", "Masters")"><i class="fa fa-user" aria-hidden="true"></i>Masters</a></li>
                                    <li class="@(controllername == "authorization" ? "active" : "")"><a href="@Url.Action("Index", "Authorization")"><i class="fa fa-lock" aria-hidden="true"></i>Authorisation</a></li>
                                    <li class="@(controllername.EndsWith("process") ? "active" : "")"><a href="@Url.Action("Index", "Process")"><i class="fa fa-cogs" aria-hidden="true"></i>Process</a></li>
                                    <li id="reports" class="@(controllername.EndsWith("reports") ? "active" : "")"><a href="@Url.Action("Index", "PrAllReports")"><i class="fa fa-file-text-o" aria-hidden="true"></i>Reports</a></li>
                                    <li class="@(controllername == "customization" ? "active" : "")"><a href="@Url.Action("Loan_Master", "Customization")"><i class="fa fa-pencil-square-o" aria-hidden="true"></i>Customization</a></li>
                                    @*  <li class="@(controllername == "PayslipPdf" ? "active" : "")"><a href="@Url.Action("Index", "PayslipPdf")"><i class="fa fa-pencil-square-o" aria-hidden="true"></i>Payslip</a></li>*@
                                    @* <li class="@(controllername == "Downloadpdf" ? "active" : "")"><a href="@Url.Action("HTMLToPDF", "Downloadpdf")"><i class="fa fa-pencil-square-o" aria-hidden="true"></i>Payslip</a></li>*@
                                    @* <li class="@(controllername == "POC" ? "active" : "")"><a href="@Url.Action("POC1", "POC")"><i class="fa fa-home" aria-hidden="true"></i>POC</a></li>*@
                                    @* <li class="@(controllername == "PayslipPdf" ? "active" : "")"><a href="@Url.Action("Mainmethod", "PayslipPdf")"><i class="fa fa-pencil-square-o" aria-hidden="true"></i>DownloadPdf</a></li>*@
                                </ul>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container-fluid">
        <div class="body-content">
            @RenderBody()
        </div>
    </div>

    <div class="footer">
        <div class="wthree_footer_copy">
            <p>© 2017 Telangana State Cooperative Apex Bank Ltd. All rights reserved </p>
        </div>
    </div>

    @*@Scripts.Render("~/bundles/jqueryval")*@
    @RenderSection("scripts", false)
    <script src="~/areas/payroll/assets/libs/jquery/payrollmenu.js"></script>

    <div id="gHrmModal" class="modal fade" role="dialog" data-keyboard="true" tabindex="-1">
        <div class="modal-dialog modal-dialog2">

            <!-- Modal content-->
            <div class="modal-content modal-content2">
                <div class="modal-header modal-header2">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h5 class="modal-title">Hello</h5>
                </div>
                <div class="modal-body">
                    <h4> Please Select Leave Type</h4>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>

        </div>
    </div>


    <div id="gHrmModal_For_MonthEnd" class="modal fade" role="dialog" data-keyboard="true" tabindex="-1">
        <div class="modal-dialog modal-dialog2">

            <!-- Modal content-->
            <div class="modal-content modal-content2">
                <div class="modal-header modal-header2">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h5 class="modal-title">Hello</h5>
                </div>
                <div class="modal-body">
                    <h4> Please Select Leave Type</h4>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" onclick="LogoutApplication();">OK</button>
                </div>
            </div>

        </div>
    </div>


    <div class="modal fade" id="confirmModal" role="dialog" data-keyboard="true" tabindex="-1">
        <div class="modal-dialog">
            <div class="modal-body" id="confirmMessage">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success ok" id="confirmOk">Ok</button>
                <button type="button" class="btn btn-default" id="confirmCancel">Cancel</button>
            </div>
        </div>
    </div>

</body>
</html>