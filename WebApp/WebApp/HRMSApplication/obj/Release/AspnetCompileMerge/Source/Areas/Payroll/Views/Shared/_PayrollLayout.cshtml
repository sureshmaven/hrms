    <link href='//fonts.googleapis.com/css?family=Lato:400,100,100italic,300,300italic,400italic,700,700italic,900,900italic'
          rel='stylesheet' type='text/css'>
    <!-- start scripts -->
    <script src="~/Areas/Payroll/Assets/libs/jquery/jquery-1.10.2.min.js"></script>
    @*<script type="text/javascript" src="~/Areas/Payroll/Assets/libs/jquery/bootstrap.js"></script>*@
    <script type="text/javascript" src="~/Areas/Payroll/Assets/libs/jquery/bootstrap.min.js"></script>
