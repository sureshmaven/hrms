    <script type="text/javascript" src="~/Areas/Payroll/Assets/libs/jquery/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Areas/Payroll/Assets/libs/jquery/datatables.buttons.min.js"></script>
    <script type="text/javascript" src="~/Areas/Payroll/Assets/libs/jquery/buttons.html5.min.js"></script>
