        } else if (empAll == true && empId == "") {
            var dataToSend = JSON.stringify({
                flag: empAll,
                final: Finalprocess
            });
            CommonApiPOST("/Payroll/Process/processTDSForAll", dataToSend, function (isSuccess) {
