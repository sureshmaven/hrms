@{
    ViewBag.Title = "TSum";
    Layout = "~/Areas/Payroll/Views/Shared/_PayrollLayout.cshtml";
}
<div class="page">
    <div class="form-horizontal form-horizontal2">
        <div class="page_start">
            <div class="col-md-2">
                @{Html.RenderPartial("_PVPayrollMenu");}
            </div>
            <div class="col-md-10">
                <div class="main_heading">
                    <h1>TSum<span class="year-end">TGCAB 01-04-2018 - 31-03-2019</span></h1>
                    <div class="col-md-4 mt_10">
                        <div class="main_heading">
                            <div class="col-md-6">
                                <input id="file" type="radio" name="file" value="1"><label for="currentmonth"><span><span></span></span>Current Month</label>
                                <div class="clearfix"></div>
                                <input id="file" type="radio" name="file" value="2"><label for="monthly"><span><span></span></span>Monthly</label>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 mt_10">
                        <input id="file" type="radio" name="file" value="1"><label for="regularemployee"><span><span></span></span>Regular Employeee</label>
                        <div class="clearfix"></div>
                        <input id="file" type="radio" name="file" value="1"><label for="supplementaryemployee"><span><span></span></span>Supplementary Employee</label>
                    </div>
                    <div class="col-md-4 mt_10">
                        <div class="form-group">
                            <label class="col-sm-5 control-label">From Date:</label>
                            <div class="col-sm-6 input-group date">
                                <input type="text" class="form-control"><span class="input-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-5 control-label">To Date:</label>
                            <div class="col-sm-6 input-group date">
                                <input type="text" class="form-control"><span class="input-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>
                            </div>
                        </div>
                        <div class="col-md-11">
                            <div class="pull-right">
                                <button type="reset" class="btn btn-default cancel">Close</button>
                                <button type="submit" class="btn btn-primary">OK</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



