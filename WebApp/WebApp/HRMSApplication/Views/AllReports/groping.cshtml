<script type="text/javascript">

    $(document).ready(function () {
        $.noConflict();
        var groupColumn = 2;

        var table = $('#example').DataTable({
            "columnDefs": [
                { "visible": false, "targets": groupColumn }
            ],
            dom: 'Bfrtip',

            buttons: [
                {
                    //text: 'PDF',
                    //extend: 'pdf',
                    //footer: true,
                    //header: true,
                    //orientation: 'portrait',
                    extend: 'pdfHtml5',
                    text: '<i class="fa fa-file-pdf-o dt-export-icon-pdf"></i>',
                    className: 'dt-export-btn',
                    titleAttr: 'Pdf',
                    title: 'TGCAB - Title',
                    messageTop: 'filter : nil',
                    customize: function (doc) {
                      
                        doc.styles.title = {
                            fontSize: '20',
                            alignment: 'center'
                        };
                        doc.styles.message = {
                            fontSize: '9',
                            alignment: 'center'
                        };
                        doc.styles.tableHeader.alignment = 'center';
                        //doc.content.splice(1, 0, {
                        //    margin: [0, -50, 0, 10],
                        //    alignment: 'left',
                        //    image: 'data:image/jpeg;base64,'
                        //});
                        //doc.content[0].text = doc.content[0].text.trim();
                        doc.content[0].text = 'ABCD';
                        

                        var lastColX = null;
                        var lastColY = null;
                        var bod = []; // this will become our new body (an array of arrays(lines))
                        //Loop over all lines in the table
                        // api.column(groupColumn, { page: 'current' }).data().each(function (line, i) {
                        var oldOffice = '';
                       
                        doc.content[2].table.body.forEach(function (line, i) {
                            //Group based on first column (ignore empty cells)
                            if (lastColX != line[2].text && line[2].text != '' && oldOffice != line[2].text) {
                                //Add line with group header
                                bod.push([{ text: line[2].text, style: 'tableHeader' }, '', '', '', '']);
                                //Update last
                                // lastColX = line[2].text;
                                oldOffice = line[2].text;
                            }
                            //Add line with data except grouped data
                            if (i < doc.content[2].table.body.length - 1) {
                                bod.push([{ text: line[0].text, style: 'defaultStyle' },
                                    { text: line[1].text, style: 'defaultStyle' },
                                    '',
                                    { text: line[3].text, style: 'defaultStyle' },
                                    { text: line[4].text, style: 'defaultStyle' }]);
                            }
                            ////Make last line bold, blue and a bit larger
                            //else {
                            //    bod.push(['', '', { text: line[2].text, style: 'lastLine' },
                            //        { text: line[3].text, style: 'lastLine' },
                            //        { text: line[4].text, style: 'lastLine' }]);
                            //}
                        });
                        //Overwrite the old table body with the new one.
                        // doc.content[2].table.headerRows = 3;
                        doc.content[2].table.widths = [125, 125, 0, 100, 100];
                        doc.content[2].table.body = bod;
                        doc.content[2].layout = 'lightHorizontalLines';

                        doc['footer'] = (function (page, pages) {
                            return {
                                columns: [
                                    'By: @ViewBag.LoginUserName , @ViewBag.LoginBranchName .',
                                    {
                                        alignment: 'right',
                                        text: [
                                            new Date().toLocaleDateString("en-IN", { day: "2-digit", month: "2-digit", year: "numeric", hour: '2-digit', minute: '2-digit' }),
                                            '    Page ', { text: page.toString() }, ' of ', { text: pages.toString() }
                                        ]
                                    }
                                ],
                                fontSize: '9',
                                margin: [40, 10]
                            }
                        });

                        doc.styles = {
                            subheader: {
                                fontSize: 10,
                                bold: true,
                                color: 'black'
                            },
                            tableHeader: {
                                bold: true,
                                fontSize: 11,
                                color: 'black',
                                background: '#DCDCDC'
                            },
                            lastLine: {
                                bold: true,
                                fontSize: 11,
                                color: 'blue'
                            },
                            defaultStyle: {
                                fontSize: 10,
                                color: 'black'
                            }
                        }
                    }
                }
            ],
            "order": [[groupColumn, 'asc']],
            "displayLength": 25,
            "drawCallback": function (settings) {
                var api = this.api();
                var rows = api.rows({ page: 'current' }).nodes();
                var last = null;

                api.column(groupColumn, { page: 'current' }).data().each(function (group, i) {
                    if (last !== group) {
                        $(rows).eq(i).before(
                            '<tr class="group"><td colspan="5">' + group + '</td></tr>'
                        );

                        last = group;
                    }
                });
            }
        });



        // Order by the grouping
        $('#example tbody').on('click', 'tr.group', function () {
            var currentOrder = table.order()[0];
            if (currentOrder[0] === groupColumn && currentOrder[1] === 'asc') {
                table.order([groupColumn, 'desc']).draw();
            }
            else {
                table.order([groupColumn, 'asc']).draw();
            }
        });
    });

</script>
