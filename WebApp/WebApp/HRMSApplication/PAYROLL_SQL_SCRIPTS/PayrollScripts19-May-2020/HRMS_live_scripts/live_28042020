update EmpLeaveBalance set LeaveBalance=(	(select LeaveBalance from EmpLeaveBalance where empid=(select id from employees where empid=	6428	and LeaveTypeId=12) and year=2020)+8) where LeaveTypeId=12 and empid=(select id from employees where empid=	6428	) and year=2020;
