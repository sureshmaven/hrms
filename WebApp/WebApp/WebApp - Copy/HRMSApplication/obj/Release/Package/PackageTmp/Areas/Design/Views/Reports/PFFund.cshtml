
@{
    ViewBag.Title = "PFFund";
    Layout = "~/Areas/Payroll/Views/Shared/_PayrollLayout.cshtml";
}

@{
    ViewBag.Title = "LICReport";
    Layout = "~/Areas/Payroll/Views/Shared/_PayrollLayout.cshtml";
}
<div class="page">
    <div class="form-horizontal form-horizontal2">
        <div class="page_start">
            <div class="col-md-2">
                @{Html.RenderPartial("_PVPayrollMenu");}
            </div>
            <div class="col-md-10">
                <div class="main_heading">
                    <h1>PF Fund<span class="year-end">TSCAB 01-04-2018 - 31-03-2019</span></h1>
                    <div class="col-md-3">
                        <div class="main_heading">
                            <input id="file" type="radio" name="file" value="1"><label for="fortheyear"><span><span></span></span>For the Year</label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="inner_heading">
                            <h1>Financial Period</h1>
                            <div class="table-responsive month_name_tbl">
                                <table id="month_name" class="display" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th>Select Financial Period</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>01-04-2018 - 31-03-2019</td>
                                        </tr>
                                        <tr>
                                            <td>01-04-2017 - 31-03-2016</td>
                                        </tr>
                                        <tr>
                                            <td>01-04-2016 - 31-03-2015</td>
                                        </tr>
                                        <tr>
                                            <td>01-04-2015 - 31-03-2014</td>
                                        </tr>
                                        <tr>
                                            <td>01-04-2014 - 31-03-2013</td>
                                        </tr>
                                        <tr>
                                            <td>01-04-2013 - 31-03-2012</td>
                                        </tr>
                                        <tr>
                                            <td>01-04-2012 - 31-03-2011</td>
                                        </tr>
                                        <tr>
                                            <td>01-04-2011 - 31-03-2010</td>
                                        </tr>
                                        <tr>
                                            <td>01-04-2010 - 31-03-2009</td>
                                        </tr>
                                        <tr>
                                            <td>01-04-2009 - 31-03-2008</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="mt_10">
                            <div class="inner_heading">
                                <h1>Employees</h1>
                                <input id="allemp" type="radio" name="allemp" value="1"><label for="allemp"><span><span></span></span>Regular Employees</label>
                                <div class="clearfix"></div>
                                <input id="allemp" type="radio" name="allemp" value="2"><label for="allemp"><span><span></span></span>Retired Employees</label>
                            </div>
                        </div>
                        <div class="mt_10">
                            <div class="inner_heading">
                                <h1>Employees</h1>
                                <input id="file" type="radio" name="file" value="1"><label for="file"><span><span></span></span>ST Employees</label>
                                <div class="clearfix"></div>
                                <input id="print" type="radio" name="file" value="2"><label for="print"><span><span></span></span>LT Employees</label>
                                <div class="clearfix"></div>
                                <input id="print" type="radio" name="file" value="3"><label for="print"><span><span></span></span>Both[ST & LT]</label>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="pull-right">
                            <button type="reset" class="btn btn-default cancel">Close</button>
                            <button type="submit" class="btn btn-primary">Submit</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        $('#month_name').DataTable({
            destroy: true,
            "paging": false,
            "ordering": false,
            "info": false,
            "bFilter": false,
        });
    });
</script>

