            reportMsgTop = 'Employee  : ' + emp_code + '  Financial Period  :   ' + fYear + '   Increment Type  :  ' + inctypeText;

            $('#rptGrid').DataTable().ajax.url(ajaxurl).load(function () {
                ajaxSpinnerHide();
            });
        }
    });
</script>






