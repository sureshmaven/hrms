<script type="text/javascript">
            $(document).ready(function () {
                $(".date").datepicker().datepicker("setDate", new Date());

                var todayTimeStamp =  new Date;
                var oneDayTimeStamp = 1000 * 60 * 60 * 24;
                var diff = todayTimeStamp - oneDayTimeStamp;
                var yesterdayDate = new Date(diff);
                var startDate = yesterdayDate.getDate() + '/' + (yesterdayDate.getMonth() + 1) + '/' + yesterdayDate.getFullYear();

                $(".pdate").datepicker().datepicker("setDate", startDate);

                //Default for todaty date in calender
                var difftoday = todayTimeStamp;
                var todayDate = new Date(difftoday);
                var startDatetoday = yesterdayDate.getDate() + '/' + (yesterdayDate.getMonth() + 1) + '/' + yesterdayDate.getFullYear();
                $("#futdatedis").datepicker().datepicker("setDate", startDatetoday); 
                $(".futdatehide").datepicker().datepicker("setDate", startDatetoday); 
            });
</script>
