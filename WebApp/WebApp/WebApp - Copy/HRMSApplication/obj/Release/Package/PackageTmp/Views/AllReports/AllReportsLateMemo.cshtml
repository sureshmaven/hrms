<script>
    $(document).ready(function () {
      
        var i = $("input[type=radio][value='True']:checked").val();
        if (i == "True") {
            $("#selfbranch").show();
            $("#Allbranch").hide();
            //textbox
            $("#SelfEmpid").show();
            $("#AllEmpid").hide();
        }
        else {
            $("#Allbranch").show();
            $("#selfbranch").hide();
            //textbox
            $("#AllEmpid").show();
            $("#SelfEmpid").hide();
        }
        });
    $('input[type=radio][name=numbers]').change(function () {
        if (this.value == 'True') {
          
            $("#selfbranch").show();
            $("#Allbranch").hide();
             //textbox
            $("#SelfEmpid").show();
            $("#AllEmpid").hide();
        }
        else if (this.value == 'False') {
       
            $('#selfbranch').val()=='';
            $("#Allbranch").show();
            $("#selfbranch").hide();

             //textbox
            $('#SelfEmpid').val() == '';
            $("#AllEmpid").show();
            $("#SelfEmpid").hide();
        }
    });
</script>


