<style>
    .reports-grids {
        width: 100% !important;
    }

    .rptGrid th {
        width: 100% !important;
    }

    .sub-menu {
        display: none;
    }

    .nav-tabs {
        border-bottom: 2px solid #DDD;
    }

        .nav-tabs > li.active > a, .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a:hover {
            border-width: 0;
        }

        .nav-tabs > li > a {
            border: none;
            color: #ffffff;
            background: #116b06;
            padding: 0 0 0px 0px;
            margin-bottom: 4px;
        }

            .nav-tabs > li.active > a, .nav-tabs > li > a:hover {
                border: none;
                color: #fff !important;
                background: #f6b659
            }

            .nav-tabs > li > a::after {
                content: "";
                background: #116b06;
                height: 2px;
                position: absolute;
                width: 100%;
                left: 0px;
                bottom: -1px;
                transition: all 250ms ease 0s;
                transform: scale(0);
            }

        .nav-tabs > li.active > a::after, .nav-tabs > li:hover > a::after {
            transform: scale(1);
        }

    .tab-nav > li > a::after {
        background: #116b06 none repeat scroll 0% 0%;
        color: #fff;
    }

    .tab-pane {
        padding: 0px 0;
    }

    .tab-content {
        padding: 20px
    }

    .nav-tabs > li {
        width: 10%;
        text-align: center;
    }

    .card {
        background: #FFF none repeat scroll 0% 0%;
        box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.3);
        margin-bottom: 30px;
    }

    .tab-content {
        background: #fff;
    }

    .mis-menus ul {
    }

        .mis-menus ul li {
            float: left;
            margin-bottom: 10px;
        }

            .mis-menus ul li a {
                padding: 0px 10px 0 10px;
                border-right: 1px solid #116b06;
                color: #000;
            }

                .mis-menus ul li a:hover {
                    color: #ec910b;
                }
</style>
